{
  "name": "parkly",
  "version": "5.0.0",
  "description": "Parkly is a parking management system",
  "main": "./out/main/index.js",
  "author": "example.com",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\"",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev-win": "chcp 65001 && electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@node-escpos/core": "^0.6.0",
    "@node-escpos/usb-adapter": "^0.3.1",
    "@tailwindcss/vite": "^4.0.14",
    "axios": "^1.8.3",
    "better-sqlite3": "^11.9.0",
    "cors": "^2.8.5",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "express-xml-bodyparser": "^0.3.0",
    "file-saver": "^2.0.5",
    "http": "^0.0.1-security",
    "node-cron": "^3.0.3",
    "pdf-to-printer": "^5.6.0",
    "pdfkit": "^0.16.0",
    "ping": "^0.4.4",
    "pinia": "^3.0.1",
    "qr-image": "^3.2.0",
    "qrcode": "^1.5.4",
    "radix-vue": "^1.9.17",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.0.14",
    "vue-toastification": "^2.0.0-rc.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@iconify/vue": "^4.3.0",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tailwindcss/postcss": "^4.0.14",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "electron": "^35.5.1",
    "electron-builder": "^24.3.0",
    "electron-vite": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "style-loader": "^4.0.0",
    "vite": "^5.0.12",
    "vue": "^3.4.15"
  },
  "build": {
    "appId": "com.kodyatemirov.parkly",
    "productName": "Parkly",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "resources/logo.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "resources/logo.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "resources/logo.ico"
    },
    "publish": null
  }
}
